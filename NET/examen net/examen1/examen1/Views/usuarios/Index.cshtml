            @Html.DisplayFor(modelItem => item.direccion_usuario)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.mail_usuario)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.rut_usuario }) |
            @Html.ActionLink("Details", "Details", new { id=item.rut_usuario }) |
